{
    "pipeline": {
        "input": [
            "img"
        ],
        "output": [
            "post_output"
        ],
        "tasks": [
            {
                "type": "Task",
                "module": "Transform",
                "name": "Preprocess",
                "input": [
                    "img"
                ],
                "output": [
                    "prep_output"
                ],
                "transforms": [
                    {
                        "type": "LoadImageFromFile"
                    },
                    {
                        "keep_ratio": true,
                        "type": "Resize",
                        "size": [
                            512,
                            512
                        ]
                    },
                    {
                        "type": "Normalize",
                        "mean": [
                            123.675,
                            116.28,
                            103.53
                        ],
                        "std": [
                            58.395,
                            57.12,
                            57.375
                        ],
                        "to_rgb": true
                    },
                    {
                        "type": "ImageToTensor",
                        "keys": [
                            "img"
                        ]
                    },
                    {
                        "type": "Collect",
                        "keys": [
                            "img"
                        ],
                        "meta_keys": [
                            "img_shape",
                            "pad_shape",
                            "ori_shape",
                            "img_norm_cfg",
                            "scale_factor"
                        ]
                    }
                ]
            },
            {
                "name": "segformer",
                "type": "Task",
                "module": "Net",
                "is_batched": true,
                "input": [
                    "prep_output"
                ],
                "output": [
                    "infer_output"
                ],
                "input_map": {
                    "img": "input"
                },
                "output_map": {}
            },
            {
                "type": "Task",
                "module": "mmseg",
                "name": "postprocess",
                "component": "ResizeMask",
                "params": {
                    "align_corners": false,
                    "channels": 256,
                    "dropout_ratio": 0.1,
                    "in_channels": [
                        64,
                        128,
                        320,
                        512
                    ],
                    "in_index": [
                        0,
                        1,
                        2,
                        3
                    ],
                    "loss_decode": {
                        "loss_weight": 1.0,
                        "type": "CrossEntropyLoss",
                        "use_sigmoid": false
                    },
                    "norm_cfg": {
                        "requires_grad": true,
                        "type": "BN"
                    },
                    "num_classes": 2,
                    "type": "SegformerHead",
                    "with_argmax": true
                },
                "output": [
                    "post_output"
                ],
                "input": [
                    "prep_output",
                    "infer_output"
                ]
            }
        ]
    }
}